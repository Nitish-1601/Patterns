 #include<stdio.h>
 #include<stdlib.h>
void main()
{
    int i,j,a=-1,n;
    char k;
    scanf("%d",&n);
    for(i = 1;i <= n;i++){
            a+=2;
            k = 'A';
        for(j = 1;j <= 51;j++){
             if(j<=a){

            printf("%c",k);
            j<i?k++:k--;
             }
        }
    printf("\n");
    }
}
