 #include<stdio.h>
 #include<stdlib.h>
void main()
{
    int i,j,a=-1;
    char k;
    for(i = 1;i <= 5;i++){
            a+=2;
            k = 'A';
        for(j = 1;j <= 17;j++){
             if(j<=a){

            printf("%c",k);
            j<i?k++:k--;
             }
        }
    printf("\n");
    }
}
